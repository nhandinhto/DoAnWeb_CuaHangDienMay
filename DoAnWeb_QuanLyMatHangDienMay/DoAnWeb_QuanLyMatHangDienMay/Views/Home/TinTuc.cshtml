@using DoAnWeb_QuanLyMatHangDienMay.Models;

@model List<TINTUC>

<div class="container">
    @foreach (TINTUC tt in Model)
    {
        <div class="mb-3">
            <img src="~/Content/HinhTinTuc/@tt.HINH" class="w-100"/>
            <div>
                <a href="/Home/ChiTietTinTuc/@tt.MATIN">
                    @tt.TENTIN
                </a>
            </div>
        </div>
    }
</div>